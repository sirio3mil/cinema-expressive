{
  "name": "sirio3mil/cinema-expressive",
  "description": "GraphQL API with Zend Expressive for movies management!",
  "type": "project",
  "homepage": "https://github.com/sirio3mil/cinema-expressive",
  "license": "BSD-3-Clause",
  "keywords": [
    "skeleton",
    "middleware",
    "psr",
    "psr-7",
    "psr-11",
    "psr-15",
    "zf",
    "zendframework",
    "zend-expressive"
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "zf": {
      "component-whitelist": [
        "zendframework/zend-expressive",
        "zendframework/zend-expressive-helpers",
        "zendframework/zend-expressive-router",
        "zendframework/zend-httphandlerrunner",
        "zendframework/zend-expressive-aurarouter",
        "zendframework/zend-expressive-twigrenderer"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/sirio3mil/imdb-scraper"
    }
  ],
  "support": {
    "issues": "https://github.com/zendframework/zend-expressive-skeleton/issues",
    "source": "https://github.com/zendframework/zend-expressive-skeleton",
    "rss": "https://github.com/zendframework/zend-expressive-skeleton/releases.atom",
    "slack": "https://zendframework-slack.herokuapp.com",
    "forum": "https://discourse.zendframework.com/c/questions/expressive"
  },
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.6",
    "graphaware/neo4j-php-client": "^4.8",
    "mongodb/mongodb": "^1.4",
    "moontoast/math": "^1.1",
    "ramsey/uuid-doctrine": "^1.4",
    "sirio3mil/imdb-scraper": "dev-master",
    "symfony/var-dumper": "^4.1",
    "symfony/yaml": "^4.1",
    "webonyx/graphql-php": "^0.12",
    "whoops/soap": "^1.0",
    "zendframework/zend-auradi-config": "^1.0",
    "zendframework/zend-cache": "^2.8",
    "zendframework/zend-component-installer": "^2.1",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-db": "^2.9",
    "zendframework/zend-debug": "^2.6",
    "zendframework/zend-diactoros": "^1.7",
    "zendframework/zend-expressive": "^3.0",
    "zendframework/zend-expressive-aurarouter": "^3.0",
    "zendframework/zend-expressive-helpers": "^5.0",
    "zendframework/zend-expressive-twigrenderer": "^2.0",
    "zendframework/zend-stdlib": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^2.9",
    "zendframework/zend-expressive-tooling": "^1.0",
    "zfcampus/zf-development-mode": "^3.1",
    "filp/whoops": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "test/AppTest/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "expressive": "expressive --ansi",
    "check": [
      "@cs-check",
      "@test",
      "@analyze"
    ],
    "analyze": "phpstan analyze -l max -c ./phpstan.installer.neon ./src ./config",
    "clear-config-cache": "php bin/clear-config-cache.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  }
}
